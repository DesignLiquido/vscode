var numeros = []

funcao adicionarNumeros(numero, quantidadeDeVezes) {
    para (var i = 0; i < quantidadeDeVezes; i = i + 1) {
        numeros.adicionar(numero)
    }
}

adicionarNumeros(10, 5)
adicionarNumeros(2, 5)
